<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shadowos.agrifarm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.shadowos.AgriBuddy.Chat_Activity"></activity>
        <activity android:name="com.shadowos.AgriBuddy.shadowact8" />
        <activity android:name="com.shadowos.AgriBuddy.shadowact7" />
        <activity android:name="com.shadowos.AgriBuddy.shadowact6" />
        <activity android:name="com.shadowos.AgriBuddy.shadowact5" />
        <activity android:name="com.shadowos.AgriBuddy.shadowact4" />
        <activity android:name="com.shadowos.AgriBuddy.shadowact3" />
        <activity android:name="com.shadowos.AgriBuddy.shadowact2" />
        <activity android:name="com.shadowos.AgriBuddy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>